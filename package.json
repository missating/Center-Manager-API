{
  "name": "event-manager",
  "version": "1.0.0",
  "description": "An event management system",
  "main": "index.js",
  "scripts": {
    "start:server": "nodemon  --exec babel-node server/index.js",
    "start:client": "webpack-dev-server --config client/config/webpack.dev.js --mode development",
    "test:server": "cross-env NODE_ENV=test npm run reset-migrate && cross-env NODE_ENV=test nyc mocha server/test/**.spec.js --timeout 3000 --exit --require babel-register --reporter spec",
    "reset-migrate": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "exclude": [
      "server/test/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/missating/Event-Manager.git"
  },
  "author": "Vanessa Ating",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/missating/Event-Manager/issues"
  },
  "homepage": "https://github.com/missating/Event-Manager#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.1",
    "nyc": "^11.6.0",
    "sequelize-cli": "^3.2.0",
    "supertest": "^3.0.0",
    "typescript": "^2.9.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "awesome-typescript-loader": "^5.0.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "mini-css-extract-plugin": "^0.4.0",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.3",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "sequelize": "^4.37.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "validator": "^9.4.1"
  }
}
